
public class Map {

	
	static int temp = 0;
	static int boardX = 5;
	static int boardY = 5;
	static int playerX = 0;
	static int playerY = 0;
	
	static int[][] twoDim = new int[boardX][boardY];
	
	private void drawMap() {
		
		for (int i = 0; i < 5; i++) {
			for (int j = 0; j < 5; j++)
				twoDim[i][j] = temp;
			temp += 0;
			
	}
		for (int i = 0; i < 5; i++) {
			for (int j = 0; j < 5; j++) {

				System.out.print(twoDim[i][j]);
				System.out.print(" ");
			}
			System.out.println();
			
}
	}
}
	
